{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "ignorePatterns": [
    "src/generated/**/*",
    "node_modules/",
    ".next/",
    "*.config.js"
  ],  
  "rules": {
    "react-hooks/exhaustive-deps": "warn",
    "@typescript-eslint/no-unused-vars": "warn"
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}